@model IEnumerable<DatabaseRemoto.Models.competitions>

@{
    ViewBag.Title = "Stagione";
}

<table class="table">
    <tr>
        <th>
            Stagione
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.season.season1)
            </td>
            <td>
                @Html.ActionLink("Dettagli" , "Index", "matches", new { id = item.id }, null)
            </td>
        </tr>
    }

</table>
